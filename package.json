{
  "name": "express-warn",
  "version": "0.1.0",
  "description": "ExpressJS middleware to warn (throttled) about request properties, like auth token in use that will soon expire\"",
  "main": "dist",
  "scripts": {
    "build": "npm run check-types && babel -d dist/ --extensions '.ts' --ignore '**/*.spec.ts,**/sample/*' -- src/",
    "build-via-typescript": "tsc --noEmit false",
    "test": "jest",
    "check-types": "tsc",
    "clean": "rimraf dist",
    "run-sample": "nodemon --exec ts-node src/sample.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Chris927/express-warn.git"
  },
  "keywords": [
    "express",
    "nodejs",
    "middleware"
  ],
  "author": "chris@uber5.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chris927/express-warn/issues"
  },
  "homepage": "https://github.com/Chris927/express-warn#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.0",
    "@types/throttle-debounce": "^2.1.0",
    "babel-jest": "^25.1.0",
    "express": "^4.17.1",
    "husky": "^4.2.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "express": "^3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "throttle-debounce": "^2.1.0"
  }
}
